{
  "name": "nodejs-backend-starter-project",
  "version": "1.1.2",
  "description": "A TypeScript Node.js backend project with Express and MongoDB.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:docker": "docker-compose exec -T app npm test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check \"**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,json}\"",
    "seed": "ts-node -r tsconfig-paths/register src/db/seed.ts",
    "migrate": "migrate",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "format": "npm run prettier:fix",
    "commit": "git-cz",
    "release": "standard-version",
    "generate:module": "ts-node -r tsconfig-paths/register scripts/generateModule.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@as-integrations/express5": "^1.1.0",
    "@graphql-tools/merge": "^9.0.24",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-requests-logger": "^4.0.3",
    "fs-extra": "^11.3.0",
    "graphql": "^16.11.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "mongoose-delete": "^1.0.2",
    "nodemailer": "^7.0.3",
    "redis": "^5.5.6",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xss": "^1.0.15",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.13.8",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-requests-logger": "^4.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose-delete": "^1.0.6",
    "@types/node": "^24.0.4",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "commitizen": "^4.3.0",
    "cross-fetch": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^30.0.2",
    "lint-staged": "^15.2.0",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.6.0",
    "react": "^19.1.0",
    "socket.io-client": "^4.8.1",
    "standard-version": "^9.5.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-migrate-mongoose": "^4.0.9",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
