{
  "name": "nodejs-backend-starter-project",
  "version": "1.0.0",
  "description": "A TypeScript Node.js backend project with Express and MongoDB.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check \"**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"**/*.{ts,json}\"",
    "test": "jest",
    "seed": "ts-node -r tsconfig-paths/register src/db/seed.ts",
    "migrate": "migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "mongoose-delete": "^1.0.2",
    "redis": "^5.5.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose-delete": "^1.0.6",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "jest": "^30.0.2",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "ts-migrate-mongoose": "^4.0.9",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
